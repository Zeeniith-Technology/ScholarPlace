        "problem_id": "Q003",
        "metadata": {
            "week": 1,
            "day": 1,
            "topic": "Variables",
            "title": "Swap Two Numbers",
            "difficulty_level": "EASY",
            "difficulty_stars": 1,
            "estimated_time_minutes": 10,
